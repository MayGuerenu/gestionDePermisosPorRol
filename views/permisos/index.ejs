<h1>Lista de permisos</h1>

<a href="/permisos/nuevo">Nuevo permiso</a>

<ul>
  <% permisos.forEach(p => { %>
    <li>
      <%= p.nombre %>
      <a href="/permisos/<%= p.id %>/editar">Editar</a>
      <form action="/permisos/<%= p.id %>/eliminar" method="POST" style="display:inline">
        <button type="submit">Eliminar</button>
      </form>
    </li>
  <% }) %>
</ul>
