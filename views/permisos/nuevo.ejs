<h1>Crear nuevo permiso</h1>

<% if (typeof error !== 'undefined') { %>
  <p style="color:red"><%= error %></p>
<% } %>

<form method="POST" action="/permisos/nuevo">
  <input type="text" name="nombre" placeholder="Nombre del permiso" value="<%= nombre || '' %>">
  <button type="submit">Guardar</button>
</form>
