<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title>Usuarios</title>
</head>
<body>
  <h1>Listado de Usuarios</h1>
  
  <a href="/users/new">Nuevo Usuario</a> | <a href="/users/login">Login</a>

  <ul>
    <% users.forEach(user => { %>
      <li><strong><%= user.user %></strong> — <%= user.email %> — <%= user.role_name || 'Sin rol' %></li>
    <% }) %>
  </ul>
</body>
</html>
